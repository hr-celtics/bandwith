{
  "name": "bandwith",
  "version": "1.0.0",
  "scripts": {
    "start": "node server",
    "start:dev": "nodemon server",
    "build:watch": "webpack --watch",
    "build:prod": "webpack --env.prod -p --progress",
    "client": "webpack-dev-server --env.dev --open",
    "debug": "nodemon --inspect=0.0.0.0:9229 server/index.js",
    "test": "NODE_ENV=test grunt test",
    "test:jest": "jest",
    "heroku-postbuild": "npm run build:prod && psql $DATABASE_URL -c 'CREATE EXTENSION postgis' && npm run migrate:latest && npm run seed",
    "loaddb": "grunt pgcreatedb:default",
    "migrate:latest": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "loadgis": "psql bandwith -c 'CREATE EXTENSION postgis'",
    "alldb": "npm run loaddb && npm run loadgis && npm run migrate:latest && npm run seed",
    "seed": "knex seed:run"
  },
  "author": "Hack Reactor",
  "license": "MIT",
